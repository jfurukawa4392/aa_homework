<!DOCTYPE html>
<html>
<head>
  <title>MusicApp</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<% unless current_user.nil? %>
  <p> User:
      <%= current_user.email %>
      <%= render 'shared/logout' %>
  </p>
  <br>
<% end %>
<%= yield %>

</body>
</html>
